<?xml version='1.0' encoding='utf-8'?>
<widget id="com.talleres.com" version="1.1.27" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>Mi Kia Talleres</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="http://186.71.68.154:8089/test/services/TL/Taller.svc/guardaArchivoEntrega/" />
